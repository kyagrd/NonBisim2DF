Define
  proved : (nat -> p -> p -> a -> p -> p -> prop) -> nat -> p -> p -> 
                                                                 a -> 
                                                                 p -> 
                                                                 p -> prop,
  disproved : (nat -> p -> p -> a -> p -> p -> prop) -> nat -> p -> p -> 
                                                                    a -> 
                                                                    p -> 
                                                                    p -> prop
by
  proved x1\x2\
         bisim_step_tbl 0 (plus (out x x (out x x z)) (out x x z))
          (plus
            (in x (x3\ in x (x4\ plus (out x x (out x x z)) (out x x z))))
            (out x x (match x1 x2 (out x x z))))
          (up x x) z (match x1 x2 (out x x z)).
